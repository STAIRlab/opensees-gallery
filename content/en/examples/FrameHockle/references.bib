@article{meier2014objective,
  title = {An Objective {{3D}} Large Deformation Finite Element Formulation for Geometrically Exact Curved {{Kirchhoff}} Rods},
  author = {Meier, Christoph and Popp, Alexander and Wall, Wolfgang A.},
  year = {2014},
