@article{rubin2007simplified,
  title = {A Simplified Implicit {{Newmark}} Integration Scheme for Finite Rotations},
  author = {Rubin, M.B.},
  year = {2007},
  month = jan,
  journal = {Computers \& Mathematics with Applications},
  volume = {53},
