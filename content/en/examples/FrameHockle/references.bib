@article{harsch2023total,
  title = {A Total {{Lagrangian}}, Objective and Intrinsically Locking-free {{Petrov}}\textendash{{Galerkin}} {{{\emph{SE}}}} (3) {{Cosserat}} Rod Finite Element Formulation},
  author = {Harsch, Jonas and Sailer, Simon and Eugster, Simon R.},
  year = {2023},
  month = jul,
  journal = {International Journal for Numerical Methods in Engineering},
  volume = {124},
