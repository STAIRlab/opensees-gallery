@article{eugster2023family,
  title = {A Family of Total {{Lagrangian Petrov}}\textendash{{Galerkin Cosserat}} Rod Finite Element Formulations},
  author = {Eugster, Simon R. and Harsch, Jonas},
  year = {2023},
