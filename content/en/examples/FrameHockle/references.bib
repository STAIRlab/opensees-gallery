@incollection{romano2005tangentb,
  title = {Tangent Stiffness of Elastic Continua on Manifolds},
  booktitle = {Trends and {{Applications}} of {{Mathematics}} to {{Mechanics}}},
  author = {Romano, Giovanni and Diaco, Marina and Sellitto, Carmen},
