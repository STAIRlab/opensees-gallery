@article{saritas2009numerical,
  title = {Numerical Integration of a Class of 3d Plastic-Damage Concrete Models and Condensation of 3d Stress\textendash Strain Relations for Use in Beam Finite Elements},
  author = {Saritas, Afsin and Filippou, Filip C.},
  year = {2009},
